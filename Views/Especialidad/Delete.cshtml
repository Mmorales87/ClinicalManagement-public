@model Turnos.Models.Especialidad

<h4> Â¿Estas seguro de eliminar la siguiente especialidad?</h4>
<h5>Especialidad</h5>
<hr>
<div class="row">
    <div class="col s12 m6">
        <div class="card hoverable blue-grey darken-5">
            <div class="card-content white-text">
                <dt>
                    <h6><b>@Html.DisplayNameFor(modelo => modelo.Descripcion)</b></h6>
                </dt>
                <hr><br>
                <dd>
                    @Html.DisplayFor(modelo => modelo.Descripcion)
                </dd>
            </div>
            <div class="card-action blue-grey darken-2">
                <form asp-action="Delete">
                    <input type="hidden" asp-for="IdEspecialidad" />
                    <input type="submit" value="Eliminar" class="waves-effect waves-light btn-small orange lighten-2" />
                    <a asp-action="Delete" asp-route-id="@Model.IdEspecialidad"></a>
                    <a asp-action="Index" class="right">Volver</a>
                </form>
            </div>
        </div>
    </div>
</div>

